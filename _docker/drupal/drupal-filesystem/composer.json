{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "composer/installers": "1.0.25",
        "drupal-composer/drupal-scaffold": "1.3.2",
        "cweagans/composer-patches": "1.5.0",
        "drupal/core": "8.1.3",
        "drush/drush": "8.1.3",
        "drupal/console": "1.0.0-beta2",
        "drupal/admin_toolbar": "1.15.0",
        "drupal/entity": "1.0.0-alpha3",
        "drupal/entity_storage_migrate": "1.x-dev",
        "drupal/diff": "1.0.0-alpha3",
        "drupal/token": "1.0.0-alpha2",
        "drupal/metatag": "1.0.0-beta9",
        "drupal/multiversion": "1.0.0-alpha9",
        "drupal/simple_sitemap": "2.3.0",
	"drupal/pathauto": "dev-1.x",
        "drupal/config_update": "1.1.0",
        "drupal/ctools": "3.0.0-alpha26",
        "drupal/scheduled_updates": "dev-1.x",
        "drupal/inline_entity_form": "dev-1.x",
        "drupal/workbench": "1.0-alpha2",
        "drupal/workbench_moderation": "dev-2.x",
        "drupal/workbench_email": "dev-1.x"
    },
    "require-dev": {
        "behat/mink": "1.7.1",
        "behat/mink-goutte-driver": "1.2.1",
        "jcalderonzumba/gastonjs": "^1.1@dev",
        "jcalderonzumba/mink-phantomjs-driver": "0.3.1",
        "mikey179/vfsStream": "1.6.3",
        "phpunit/phpunit": "4.8.26",
        "symfony/css-selector": "2.7.13",
        "drupal/devel": "1.x-dev"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::buildScaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::buildScaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/core": {
                "Avoid to check the render context to detect early rendering for Non-GET requests.": "2663638-2.patch"
            },
            "drupal/scheduled_updates": {
                "No canonical link for update types": "https://www.drupal.org/files/issues/scheduled_updates-no-canonical-link-for-update-type.patch"     
            }
        }
    }
}
